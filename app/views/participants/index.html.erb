<p id="notice"><%= notice %></p>

<h1>Participants</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Gender</th>
      <th>Bloog group</th>
      <th>Weight</th>
      <th>Organisation</th>
       <th>Event</th>
       <th>Catagory</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participants.each do |participant| %>
      <tr>
        <td><%= participant.name %></td>
        <td><%= participant.dob %></td>
        <td><%= participant.gender %></td>
        <td><%= participant.bloog_group %></td>
        <td><%= participant.weight %></td>
        <td><%= participant.organisation %></td>
        <td><%= participant.event %></td>
        <td><%= participant.catagory %></td>

        <td><%= link_to 'Show', participant %></td>
        <td><%= link_to 'Edit', edit_participant_path(participant) %></td>
        <td><%= link_to 'Destroy', participant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participant', new_participant_path %>
